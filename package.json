  "scripts": {
    "clean-deps": "rimraf node_modules",
    "es-check": "es-check es5 ./index.js ./_common.js ./ultimate.js ./plugins/**/*.js -v",
    "prepare": "npx gulp --gulpfile gulpPrep.ts",
    "start": "npx gulp --open",
    "build": "npx gulp build --production"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dan503/mq-js.git"
